<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.grameenphone" >

    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/MyMaterialTheme" >
        <activity
            android:name=".raj_ui.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Splashscreen"
            android:label="@string/title_activity_home" >
        </activity>
        <activity
            android:name=".activity.SelctContacts"
            android:label="@string/title_activity_selct_contacts" >
            <intent-filter>
                <action android:name="android.intent.action.SEARCH" />
            </intent-filter>

            <meta-data
                android:name="android.app.searchable"
                android:resource="@layout/searchable" />
        </activity>
        <activity
            android:name=".raj_ui.SignUpActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".activity.Grameenhome"
            android:label="@string/title_activity_grameenhome" >
        </activity>
        <activity
            android:name=".activity.TransactionOverview"
            android:label="@string/title_activity_transaction_overview" >
        </activity>
        <activity
            android:name=".activity.TransactionOverviewDeatils"
            android:label="@string/title_activity_transaction_overview_deatils" >
        </activity>
    </application>

</manifest>
